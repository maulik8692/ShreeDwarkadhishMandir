@using ShreeDwarkadhishMandir.Models;
@{
    ViewBag.Title = "ApplicationUserList";
    string casheCleaner = DateTime.Now.ToString("ddMMyyyyhhmmss");
    bool IsAllowedMandir = CheckValidation.IsAllowedMandir();
}

<div class="row PaddingTop PaddingRight PaddingLeft">
    <div class="form-group row col-lg-12" style="margin-bottom:10px;">
        <div class="col col-md-6">
            @*<input type="text" id="ReceiptNo" name="ReceiptNo" placeholder="Please Enter Name/Username/UserType" class="form-control">*@
        </div>
        @if (@IsAllowedMandir)
        {
            <div class="col col-md-6">
                <a id="reset" href="/ApplicationUser/ApplicationUser" style="float:right;" class="btn btn-primary">
                    <i class="fa fa-plus"></i> New
                </a>
            </div>
        }
    </div>
    <div class="col-lg-12">
        <div id="divReceipt" class="table-responsive table--no-card m-b-30">
            <table id="ApplicationUserList" style="width:100%" class="table table-borderless table-striped table-mandir">
            </table>
            <div id="pager"></div>
        </div>
    </div>
</div>

<script src="~/js/ApplicationUserList.js?@casheCleaner"></script>