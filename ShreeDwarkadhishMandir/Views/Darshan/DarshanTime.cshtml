@{
    ViewBag.Title = "DarshanTime";
    string casheCleaner = DateTime.Now.ToString("ddMMyyyyhhmmss");
}
<div class="row PaddingTop PaddingRight PaddingLeft">
 
    <div class="form-group row col-lg-12" style="margin-bottom:10px;">
        <div class="col col-md-2">
            <input id="DarshanDate" name="Date" type="text" placeholder="Enter Date" class="form-control">
        </div>

        <div class="col col-md-2">
            <button id="SearchDarshan" style="min-width: 20%;" type="submit" class="btn button-save-new rounded-button">
                <i class="las la-search"></i> Search
            </button>
        </div>
        <div class="col col-md-8">
            <a href="\Darshan\CreateDarshan" style="float:right;min-width: 20%;" class="btn btn-primary rounded-button"><i class="las la-plus"></i>Add</a>
        </div>
    </div>
    <div class="col-lg-12">
        <div id="divDarshanTime" class="table-responsive table--no-card m-b-30">
            <table id="DarshanTime" class="table table-borderless table-striped table-mandir"></table>
            @*<div id="pager"></div>*@
        </div>
    </div>
</div>
<script src="~/js/DarshanTime.js?@casheCleaner"></script>